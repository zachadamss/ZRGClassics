<!-- Community Resources Section -->
<section id="community-resources" class="resource-section">
    <h2 class="section-title">Community Resources</h2>
    <p class="section-subtitle">Popular YouTube channels, forums, and communities dedicated to the {{ vehicle.model }}</p>

    <div class="community-resources-grid">
        {% if vehicle.communityResources.youtube and vehicle.communityResources.youtube.length %}
        <div class="resource-category">
            <h3 class="resource-category-title">
                <svg class="resource-icon" viewBox="0 0 24 24" fill="currentColor" width="24" height="24">
                    <path d="M23.498 6.186a3.016 3.016 0 0 0-2.122-2.136C19.505 3.545 12 3.545 12 3.545s-7.505 0-9.377.505A3.017 3.017 0 0 0 .502 6.186C0 8.07 0 12 0 12s0 3.93.502 5.814a3.016 3.016 0 0 0 2.122 2.136c1.871.505 9.376.505 9.376.505s7.505 0 9.377-.505a3.015 3.015 0 0 0 2.122-2.136C24 15.93 24 12 24 12s0-3.93-.502-5.814zM9.545 15.568V8.432L15.818 12l-6.273 3.568z"/>
                </svg>
                YouTube Channels
            </h3>
            <div class="resource-list">
                {% for channel in vehicle.communityResources.youtube %}
                <a href="{{ channel.url }}" target="_blank" rel="noopener noreferrer" class="resource-item">
                    <div class="resource-item-content">
                        <span class="resource-name">{{ channel.name }}</span>
                        <span class="resource-description">{{ channel.description }}</span>
                    </div>
                    <svg class="external-link-icon" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" width="16" height="16">
                        <path d="M18 13v6a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h6M15 3h6v6M10 14L21 3"/>
                    </svg>
                </a>
                {% endfor %}
            </div>
        </div>
        {% endif %}

        {% if vehicle.communityResources.forums and vehicle.communityResources.forums.length %}
        <div class="resource-category">
            <h3 class="resource-category-title">
                <svg class="resource-icon" viewBox="0 0 24 24" fill="currentColor" width="24" height="24">
                    <path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm-1 17.93c-3.95-.49-7-3.85-7-7.93 0-.62.08-1.21.21-1.79L9 15v1c0 1.1.9 2 2 2v1.93zm6.9-2.54c-.26-.81-1-1.39-1.9-1.39h-1v-3c0-.55-.45-1-1-1H8v-2h2c.55 0 1-.45 1-1V7h2c1.1 0 2-.9 2-2v-.41c2.93 1.19 5 4.06 5 7.41 0 2.08-.8 3.97-2.1 5.39z"/>
                </svg>
                Forums
            </h3>
            <div class="resource-list">
                {% for forum in vehicle.communityResources.forums %}
                <a href="{{ forum.url }}" target="_blank" rel="noopener noreferrer" class="resource-item">
                    <div class="resource-item-content">
                        <span class="resource-name">{{ forum.name }}</span>
                        <span class="resource-description">{{ forum.description }}</span>
                    </div>
                    <svg class="external-link-icon" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" width="16" height="16">
                        <path d="M18 13v6a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h6M15 3h6v6M10 14L21 3"/>
                    </svg>
                </a>
                {% endfor %}
            </div>
        </div>
        {% endif %}

        {% if vehicle.communityResources.facebook and vehicle.communityResources.facebook.length %}
        <div class="resource-category">
            <h3 class="resource-category-title">
                <svg class="resource-icon" viewBox="0 0 24 24" fill="currentColor" width="24" height="24">
                    <path d="M24 12.073c0-6.627-5.373-12-12-12s-12 5.373-12 12c0 5.99 4.388 10.954 10.125 11.854v-8.385H7.078v-3.47h3.047V9.43c0-3.007 1.792-4.669 4.533-4.669 1.312 0 2.686.235 2.686.235v2.953H15.83c-1.491 0-1.956.925-1.956 1.874v2.25h3.328l-.532 3.47h-2.796v8.385C19.612 23.027 24 18.062 24 12.073z"/>
                </svg>
                Facebook Groups
            </h3>
            <div class="resource-list">
                {% for group in vehicle.communityResources.facebook %}
                <a href="{{ group.url }}" target="_blank" rel="noopener noreferrer" class="resource-item">
                    <div class="resource-item-content">
                        <span class="resource-name">{{ group.name }}</span>
                        <span class="resource-description">{{ group.description }}</span>
                    </div>
                    <svg class="external-link-icon" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" width="16" height="16">
                        <path d="M18 13v6a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h6M15 3h6v6M10 14L21 3"/>
                    </svg>
                </a>
                {% endfor %}
            </div>
        </div>
        {% endif %}

        {% if vehicle.communityResources.websites and vehicle.communityResources.websites.length %}
        <div class="resource-category">
            <h3 class="resource-category-title">
                <svg class="resource-icon" viewBox="0 0 24 24" fill="currentColor" width="24" height="24">
                    <path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm-1 17.93c-3.95-.49-7-3.85-7-7.93 0-.62.08-1.21.21-1.79L9 15v1c0 1.1.9 2 2 2v1.93zm6.9-2.54c-.26-.81-1-1.39-1.9-1.39h-1v-3c0-.55-.45-1-1-1H8v-2h2c.55 0 1-.45 1-1V7h2c1.1 0 2-.9 2-2v-.41c2.93 1.19 5 4.06 5 7.41 0 2.08-.8 3.97-2.1 5.39z"/>
                </svg>
                Websites & Blogs
            </h3>
            <div class="resource-list">
                {% for site in vehicle.communityResources.websites %}
                <a href="{{ site.url }}" target="_blank" rel="noopener noreferrer" class="resource-item">
                    <div class="resource-item-content">
                        <span class="resource-name">{{ site.name }}</span>
                        <span class="resource-description">{{ site.description }}</span>
                    </div>
                    <svg class="external-link-icon" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" width="16" height="16">
                        <path d="M18 13v6a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h6M15 3h6v6M10 14L21 3"/>
                    </svg>
                </a>
                {% endfor %}
            </div>
        </div>
        {% endif %}

        {% if vehicle.communityResources.other and vehicle.communityResources.other.length %}
        <div class="resource-category">
            <h3 class="resource-category-title">
                <svg class="resource-icon" viewBox="0 0 24 24" fill="currentColor" width="24" height="24">
                    <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"/>
                </svg>
                Other Resources
            </h3>
            <div class="resource-list">
                {% for resource in vehicle.communityResources.other %}
                <a href="{{ resource.url }}" target="_blank" rel="noopener noreferrer" class="resource-item">
                    <div class="resource-item-content">
                        <span class="resource-name">{{ resource.name }}</span>
                        <span class="resource-description">{{ resource.description }}</span>
                    </div>
                    <svg class="external-link-icon" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" width="16" height="16">
                        <path d="M18 13v6a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h6M15 3h6v6M10 14L21 3"/>
                    </svg>
                </a>
                {% endfor %}
            </div>
        </div>
        {% endif %}
    </div>
</section>
